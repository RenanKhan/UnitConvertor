    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
   >
    <TableRow
        android:layout_marginBottom="5dp"
        >
        <!--
        basic section Start
       -->
        <RelativeLayout
            android:id="@+id/length"
            android:onClick="onClickLisnor"
            android:layout_marginLeft="0dp"
            android:background="@drawable/roundedbox"
            android:layout_width="14mm"
            android:layout_height="14mm"
            android:clickable="true">
            <LinearLayout
                android:layout_marginLeft="3mm"
                android:layout_marginTop="1.5mm"
                android:layout_marginBottom="1.5mm"
                android:layout_marginRight="3mm"
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <ImageView
                    android:layout_weight="1"
                    android:layout_gravity="center_horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/length"
                    android:clickable="true" />
                <TextView
                    android:layout_weight="3"
                    android:textAlignment="center"
                    android:layout_marginTop="1mm"
                    android:textColor="#1e1e1e"
                    android:layout_gravity="center_horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="13dp"
                    android:text="Length"/>
            </LinearLayout>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/area"
            android:onClick="onClickLisnor"
            android:layout_marginLeft="5dp"
            android:background="@drawable/roundedbox"
            android:layout_width="14mm"
            android:layout_height="14mm"
            >
            <LinearLayout
