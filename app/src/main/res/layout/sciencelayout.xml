<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal" android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <RelativeLayout
        android:layout_marginLeft="1mm"
        android:background="@drawable/roundedbox"
        android:layout_width="14mm"
        android:layout_height="14mm"
        >
        <LinearLayout
            android:layout_marginLeft="3mm"
            android:layout_marginTop="2mm"
            android:layout_marginBottom="2mm"
            android:layout_marginRight="3mm"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_gravity="center_horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/force"/>
            <TextView
                android:textAlignment="center"
                android:layout_marginTop="1mm"
                android:textColor="#1e1e1e"
                android:layout_gravity="center_horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="15dp"
                android:text="Force"/>
        </LinearLayout>
    </RelativeLayout>
    <RelativeLayout
        android:layout_marginLeft="1mm"
        android:background="@drawable/roundedbox"
        android:layout_width="14mm"
        android:layout_height="14mm"
        >
        <LinearLayout
            android:layout_marginLeft="3mm"
            android:layout_marginTop="2mm"
            android:layout_marginBottom="2mm"
            android:layout_marginRight="3mm"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_gravity="center_horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/length"/>
            <TextView
                android:textAlignment="center"
                android:layout_marginTop="1mm"
                android:textColor="#1e1e1e"
                android:layout_gravity="center_horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="15dp"
                android:text="Work"/>
        </LinearLayout>
    </RelativeLayout>
    <RelativeLayout
        android:layout_marginLeft="1mm"
        android:background="@drawable/roundedbox"
        android:layout_width="14mm"
        android:layout_height="14mm"
        >
        <LinearLayout
            android:layout_marginLeft="3mm"
            android:layout_marginTop="2mm"
            android:layout_marginBottom="2mm"
            android:layout_marginRight="3mm"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_gravity="center_horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/power"/>
            <TextView
                android:textAlignment="center"

                android:layout_marginTop="1mm"
                android:textColor="#1e1e1e"
                android:layout_gravity="center_horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="15dp"
                android:text="Power"/>
        </LinearLayout>
    </RelativeLayout>
    <RelativeLayout
        android:layout_marginLeft="1mm"
        android:background="@drawable/roundedbox"
        android:layout_width="14mm"
        android:layout_height="14mm"
        >
        <LinearLayout
            android:layout_marginLeft="3mm"
            android:layout_marginTop="2mm"
            android:layout_marginBottom="2mm"
            android:layout_marginRight="3mm"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_gravity="center_horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/pressure"/>
            <TextView
                android:textAlignment="center"
                android:layout_marginTop="1mm"
                android:textColor="#1e1e1e"
                android:layout_gravity="center_horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="15dp"
                android:text="Pressure"/>
        </LinearLayout>
    </RelativeLayout>
   <!-- <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="sciencelayout"/>-->
</LinearLayout>